{
    "LuaCoderAssist.format.indentCount": 2,
    "LuaCoderAssist.ldoc.authorInFunctionLevel": false,
    "LuaCoderAssist.format.quotemark": "double",
    "spellright.language": [
        "en_GB"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "git-commit",
        "lua",
        "shellscript",
        "github-actions",
        "json",
        "properties"
    ],
    "editor.formatOnSave": true,
    "[dockerfile]": {
        "editor.formatOnSave": false
    },
    "[github-actions]": {
        "editor.formatOnSave": false
    },
    "spellright.parserByClass": {
        "properties": {
            "parser": "plain"
        }
    },
    "vscode-lua-format.configPath": ".luaformat",
}
